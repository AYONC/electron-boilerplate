{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "sourceMap": false,
    "importHelpers": true,
    "noEmitHelpers": true,
    "module": "commonjs",
    "target": "es2017",
    "jsx": "react",
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
